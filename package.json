{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.build.json",
    "build:deploy": "pnpm db:generate && pnpm db:migrate && pnpm db:migrate:deploy && pnpm build",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate --schema=./src/models/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./src/models/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=./src/models/schema.prisma",
    "db:push": "prisma db push --schema=./src/models/schema.prisma",
    "db:studio": "prisma studio --schema=./src/models/schema.prisma",
    "db:format": "prisma format --schema=./src/models/schema.prisma",
    "db:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "tsx ./src/models/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/validator": "^13.15.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "firebase-admin": "^13.4.0",
    "jose": "^6.0.11",
    "validator": "^13.15.15",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tsconfig/node22": "^22.0.2",
    "eslint": "^9.29.0",
    "eslint-plugin-perfectionist": "^4.14.0",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0"
  }
}
